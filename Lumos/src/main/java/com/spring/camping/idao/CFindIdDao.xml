<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.camping.login.findid.CFindIdDao">

	<select id="findId" resultType="com.spring.camping.login.findid.CUserInfoDto">
		SELECT USERID FROM USERINFO WHERE USERNAME = #{param1} AND USERTELNO = #{param2} AND USEREMAIL = #{param3}	
		</select>
										
</mapper>